# Frontend env vars (Vite)
# Toggle demo mode on locally to use built-in demo data and login fallback
VITE_USE_DEMO=true

# Base URL for the backend API (only used when VITE_USE_DEMO=false)
# Examples:
# VITE_API_BASE_URL=http://localhost:4000/api/v1
# VITE_API_BASE_URL=https://your-api.example.com/api/v1
VITE_API_BASE_URL=
